<?xml version="1.0" encoding="UTF-8"?>
<suite parallel="false" name="Suite">

  <test name="Test">
  
     <groups>
    	<run>
    		<include name="user" />
    		<exclude name="admin" />
    	</run>
    </groups>
    
    <classes>
      <class name="testNG.Topic02_TestNG_Groups"/>
    </classes>
    
  </test> 
  
  <test name ="Run on FireFox">
  	<parameter name="browser" value="firefox">
  	</parameter>
  	 <classes>
      <class name="testNG.Topic05_TestNG_Parameters_MultiBrowser"/>
    </classes>
  </test>
  
   <test name ="Run on Chrome">
  	<parameter name="browser" value="chrome">
  	</parameter>
  	 <classes>
      <class name="testNG.Topic05_TestNG_Parameters_MultiBrowser"/>
    </classes>
  </test>
  
   <test name ="Run on Edge">
  	<parameter name="browser" value="edge">
  	</parameter>
  	 <classes>
      <class name="testNG.Topic05_TestNG_Parameters_MultiBrowser"/>
    </classes>
  </test>
  
</suite> 
